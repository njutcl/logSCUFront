  <!--pages/dingdan/lishi/lishi.wxml-->
<view class="page">

 <view class="container">
  <!-- 导航栏 -->
  <view class="navbar">
    <block wx:for="{{navbarTitle}}" wx:key="index">
      <view class="navbar-item {{navbarActiveIndex === index ? 'navbar-item-active' : ''}}" data-navbar-index="{{index}}" catchtap="onNavBarTap">
        <text>{{item}}</text>
      </view> 
    </block>
  </view>
  <view class="movie-content-wrapper">
    <swiper current="{{navbarActiveIndex}}" bindanimationfinish="onBindAnimationFinish" style='height:100%'>
      <swiper-item>
        <block wx:if="{{!flag[0]}}">
          <view class='box-container'>
            <image class='box-blankIcon' src='/img/order/blank.png'></image>
            <text class='box-blankText'>您目前暂无订单哦~</text>
          </view>
        </block>
        <block wx:if="{{flag[0]}}">
          <scroll-view scroll-y="{{true}}" style="height:{{scrollHeight}}px">
            <block wx:for="{{list.uncompleted}}" wx:for-item="item" wx:for-index="index" wx:key="{{item.orderid}}">
              <view class='box-list'>
                <image class='order-img' src='{{item.order_owner.head_img}}'></image>
                <view class='order-name' data-index="{{index}}" bindtap='navigateToDetail'>
                  <text class='order-wxName'>{{item.order_owner.wx_name}}</text>
                  <view class='order-arrow'></view>
                </view>
                <view class='order-express'>
                  <text>{{item.kuaidi}}</text>
                  <text>x1</text>
                </view>
                <view class='order-line'></view>
                <view class='order-money'>共1件快递，实付
                  <text class='order-moneyText'>¥{{item.money}}</text>
                </view>
                <view class='order-button'>
             <button class="btn btn1" bindtap='toReceiveOrder'>去接单</button>
                  <button class="btn btn2" bindtap='toSendOrder'>去发单</button>
                </view>
              </view>
            </block>
          </scroll-view>
        </block>
      </swiper-item>
      <swiper-item>
        <block wx:if="{{!flag[1]}}">
          <view class='box-container'>
            <image class='box-blankIcon' src='/img/order/blank.png'></image>
            <text class='box-blankText'>您目前暂无订单哦~</text>
          </view>
        </block>
        <block wx:if="{{flag[1]}}">
          <scroll-view scroll-y="{{true}}" style="height:{{height}}px">
            <block wx:for="{{list.completing}}" wx:for-item="item" wx:for-index="index" wx:key="{{item.orderid}}">
              <view class='box-list'>
                <image class='order-img' src='{{item.order_owner.head_img}}'></image>
                <view class='order-name' data-index="{{index}}" data-status="{{item.status}}" bindtap='navigateToDetail'>
                  <text class='order-wxName'>{{item.order_owner.wx_name}}</text>
                  <view class='order-arrow'></view>
                </view>
                <view class='order-express'>
                  <text>{{item.kuaidi}}</text>
                  <text>x1</text>
                </view>
                <view class='order-line'></view>
                <view class='order-money'>共1件快递，实付
                  <text class='order-moneyText'>¥{{item.money}}</text>
                </view>
                <view class='order-button'>
                       <button class="btn btn1" data-orderid='{{item.orderid}}' bindtap='cancelOrder'>取消订单</button>
                  <button class="btn btn2" data-orderid='{{item.orderid}}' bindtap='completeOrder'>确认收货</button>
                </view>
              </view>
            </block>
          </scroll-view>
        </block>
      </swiper-item>
      <swiper-item>
        <block wx:if="{{!flag[2]}}">
          <view class='box-container'>
            <image class='box-blankIcon' src='/img/order/blank.png'></image>
            <text class='box-blankText'>您目前暂无订单哦~</text>
          </view>
        </block>
        <block wx:if="{{flag[2]}}">
          <scroll-view scroll-y="{{true}}" style="height:{{scrollHeight}}px">
            <block wx:for="{{list.uncommented}}" wx:for-item="item" wx:for-index="index" wx:key="{{item.orderid}}">
              <view class='box-list'>
                <image class='order-img' src='{{item.order_owner.head_img}}' wx:if="{{item.order_owner}}"></image>
                <image class='order-img' src='{{defaultImg}}' wx:else></image>
                <view class='order-name' data-index="{{index}}" data-status="{{item.status}}" bindtap='navigateToDetail'>
                  <text class='order-wxName' wx:if="{{item.order_owner}}">{{item.order_owner.wx_name}}</text>
                  <text class='order-wxName' wx:else>未接单</text>
                  <view class='order-arrow'></view>
                </view>
                <view class='order-express'>
                  <text>{{item.kuaidi}}</text>
                  <text>x1</text>
                </view>
                <view class='order-line'></view>
                <view class='order-money'>共1件快递，实付
                  <text class='order-moneyText'>¥{{item.money}}</text>
                </view>
                <view class='order-button'>
                           <button class="btn btn1" data-orderid='{{item.orderid}}' data-ownerimg='{{item.order_owner.head_img}}'data-lancerimg='{{item.free_lancer.head_img}}' bindtap='getComment'>查看评价</button>
                  <button class="btn btn2" data-orderid='{{item.orderid}}'   bindtap='toComment'>评价</button>
                </view>
              </view>
            </block>
          </scroll-view>
        </block>
      </swiper-item>
      <swiper-item>
        <block wx:if="{{!flag[3]}}">
          <view class='box-container'>
            <image class='box-blankIcon' src='/img/order/blank.png'></image>
            <text class='box-blankText'>您目前暂无订单哦~</text>
          </view>
        </block>
        <block wx:if="{{flag[3]}}">
          <scroll-view scroll-y="{{true}}" style="height:{{height}}px">
            <block wx:for="{{list.completed}}" wx:for-item="item" wx:for-index="index" wx:key="{{orderid}}">
                          <view class='box-list'>
                <image class='order-img' src='{{item.order_owner.head_img}}' wx:if="{{item.order_owner}}"></image>
                <image class='order-img' src='{{defaultImg}}' wx:else></image>
                <view class='order-name' data-index="{{index}}" data-status="{{item.status}}" bindtap='navigateToDetail'>
                  <text class='order-wxName' wx:if="{{item.order_owner}}">{{item.order_owner.wx_name}}</text>
                  <text class='order-wxName' wx:else>未接单</text>
                  <view class='order-arrow'></view>
                </view>
                <view class='order-express'>
                  <text>{{item.kuaidi}}</text>
                  <text>x1</text>
                </view>
                <view class='order-line'></view>
                <view class='order-money'>共1件快递，实付
                  <text class='order-moneyText'>¥{{item.money}}</text>
                </view>
                <view class='order-button'>
       <button class="btn btn1" data-orderid='{{item.orderid}}' data-ownerimg='{{item.order_owner.head_img}}'data-lancerimg='{{item.free_lancer.head_img}}' bindtap='getComment'>查看评价</button>
                  
                </view>
              </view>
            </block>
          </scroll-view>
        </block>
      </swiper-item>
      <swiper-item>
        <block wx:if="{{!flag[4]}}">
          <view class='box-container'>
            <image class='box-blankIcon' src='/img/order/blank.png'></image>
            <text class='box-blankText'>您目前暂无订单哦~</text>
          </view>
        </block>
        <block wx:if="{{flag[4]}}">
          <scroll-view scroll-y="{{true}}" style="height:{{scrollHeight}}px">
            <block wx:for="{{list.canceled}}" wx:for-item="item" wx:for-index="index" wx:key="{{item.orderid}}">
              <view class='box-list'>
                <image class='order-img' src='{{item.order_owner.head_img}}'></image>
                <view class='order-name' data-index="{{index}}" bindtap='navigateToDetail'>
                  <text class='order-wxName'>{{item.order_owner.wx_name}}</text>
                  <view class='order-arrow'></view>
                </view>
                <view class='order-express'>
                  <text>{{item.kuaidi}}</text>
                  <text>x1</text>
                </view>
                <view class='order-line'></view>
                <view class='order-money'>共1件快递，实付
                  <text class='order-moneyText'>¥{{item.money}}</text>
                </view>
                <view class='order-button'>
                             <button class="btn btn1" bindtap='toReceiveOrder'>去接单</button>
                  <button class="btn btn2" bindtap='toSendOrder'>去发单</button>
                </view>
              </view>
            </block>
          </scroll-view>
        </block>
      </swiper-item>
            <swiper-item>
        <block wx:if="{{!flag[5]}}">
          <view class='box-container'>
            <image class='box-blankIcon' src='/img/order/blank.png'></image>
            <text class='box-blankText'>您目前暂无订单哦~</text>
          </view>
        </block>
        <block wx:if="{{flag[5]}}">
          <scroll-view scroll-y="{{true}}" style="height:{{scrollHeight}}px">
            <block wx:for="{{list.expired}}" wx:for-item="item" wx:for-index="index" wx:key="{{item.orderid}}">
              <view class='box-list'>
                <image class='order-img' src='{{item.order_owner.head_img}}'></image>
                <view class='order-name' data-index="{{index}}" bindtap='navigateToDetail'>
                  <text class='order-wxName'>{{item.order_owner.wx_name}}</text>
                  <view class='order-arrow'></view>
                </view>
                <view class='order-express'>
                  <text>{{item.kuaidi}}</text>
                  <text>x1</text>
                </view>
                <view class='order-line'></view>
                <view class='order-money'>共1件快递，实付
                  <text class='order-moneyText'>¥{{item.money}}</text>
                </view>
                <view class='order-button'>
             <button class="btn btn1" bindtap='toReceiveOrder'>去接单</button>
                  <button class="btn btn2" bindtap='toSendOrder'>去发单</button>
                </view>
              </view>
            </block>
          </scroll-view>
        </block>
      </swiper-item>   
    </swiper>
  </view>
</view>








</view>
